    "@solana/web3.js": "^1.95.5",
    "axios": "^1.6.8",
    "bs58": "^5.0.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "joi": "^17.12.1",
    "morgan": "^1.10.0",
    "pino": "^9.1.0",
    "pino-pretty": "^10.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.12.7",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.29.1",
    "prisma": "^5.12.1",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
